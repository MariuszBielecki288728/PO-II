import java.util.LinkedList;

public class Sort implements Runnable {
	Bufor<String> buf;
	LinkedList<String> lista;
	String nazw;
	
	Sort(Bufor<String> b){
		buf = b;
		lista = new LinkedList<>();
	}

	@Override
	public void run() {
		lista.add(0, buf.get());
		
		while (buf.czyProdZakon==0){
			nazw = buf.get();
			for (int i = lista.size()-1; i>=0; i--){
				if ((lista.get(i).compareTo(nazw))>0)
				{
					lista.add(i+1, nazw);
				}
			}
		}
		
	}
}
